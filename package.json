{
  "name": "crypto-cheatsheet",
  "version": "1.0.0",
  "description": "",
  "main": "index.mjs",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "full": "npm run clean; npm run copy-static; npm run generate-content; npm run deploy",
    "clean": "rm -rf dist/*",
    "copy-static": "cp -R static/* dist/",
    "generate-content": "node build.mjs; mkdir dist/css/; npx postcss src/css/custom.css > dist/css/custom.css",
    "deploy": "aws s3 sync dist/ s3://cryptocheatsheet.ninja/ --acl public-read"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawngmc/crypto-cheatsheet.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shawngmc/crypto-cheatsheet/issues"
  },
  "homepage": "https://github.com/shawngmc/crypto-cheatsheet#readme",
  "dependencies": {
    "jsdom": "^16.7.0",
    "remark": "^14.0.1",
    "remark-html": "^14.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "snowpack": "^3.8.3"
  },
  "devDependencies": {
    "cssnano": "^5.0.7",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2"
  }
}
